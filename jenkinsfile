

pipeline {
	agent any {
	stages {
		stage ('code checkout') {
			steps {
				git 'https://github.com/inaeon/ABC.git'
			}
		}
		stage ('code compile') {
			steps {
				sh 'mvn clean compile'
			}
		}
		stage ('code test') {
			steps {
				sh 'mvn test'
			}
		}
		stage ('code package'){
			steps {
				sh 'mvn package'
			}
		}
	
	
	
	}
	}

}
